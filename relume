    <!-- Style Guide Selector -->
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            document.querySelectorAll('.dropdown-styleguide-link-relume').forEach(trigger => {
                trigger.addEventListener('click', function() {
                    document.querySelectorAll('.dropdown-styleguide-link-relume').forEach(target => target.classList.add('active'));
                    document.querySelectorAll('.dropdown-option-display-finsweet').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.finsweet-clone-button').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.dropdown-styleguide-link-finsweet').forEach(target => target.classList.remove('active'));
                    document.querySelectorAll('.dropdown-option-display-relume').forEach(target => target.classList.remove('hide'));
                    document.querySelectorAll('.relume-clone-button').forEach(target => target.classList.remove('hide'));
                });
            });

            document.querySelectorAll('.dropdown-styleguide-link-finsweet').forEach(trigger => {
                trigger.addEventListener('click', function() {
                    document.querySelectorAll('.dropdown-styleguide-link-finsweet').forEach(target => target.classList.add('active'));
                    document.querySelectorAll('.dropdown-option-display-relume').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.relume-clone-button').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.dropdown-styleguide-link-relume').forEach(target => target.classList.remove('active'));
                    document.querySelectorAll('.dropdown-option-display-finsweet').forEach(target => target.classList.remove('hide'));
                    document.querySelectorAll('.finsweet-clone-button').forEach(target => target.classList.remove('hide'));
                });
            });
        });
    </script>

    <!-- Menu Tabs -->
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            document.querySelectorAll('.navbar_dropdown-tab-link-ecommerce').forEach(trigger => {
                trigger.addEventListener('click', function() {
                    document.querySelectorAll('.search-dropdown-list-wrapper-ecommerce').forEach(target => target.classList.remove('hide'));
                    document.querySelectorAll('.search-dropdown-list-wrapper-marketing').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.search-dropdown-list-wrapper-applicationui').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-marketing').forEach(target => target.classList.remove('active'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-applicationui').forEach(target => target.classList.remove('active'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-ecommerce').forEach(target => target.classList.add('active'));

                });
            });

            document.querySelectorAll('.navbar_dropdown-tab-link-marketing').forEach(trigger => {
                trigger.addEventListener('click', function() {
                    document.querySelectorAll('.search-dropdown-list-wrapper-marketing').forEach(target => target.classList.remove('hide'));
                    document.querySelectorAll('.search-dropdown-list-wrapper-ecommerce').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.search-dropdown-list-wrapper-applicationui').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-marketing').forEach(target => target.classList.add('active'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-ecommerce').forEach(target => target.classList.remove('active'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-applicationui').forEach(target => target.classList.remove('active'));
                });
            });

            document.querySelectorAll('.navbar_dropdown-tab-link-applicationui').forEach(trigger => {
                trigger.addEventListener('click', function() {
                    document.querySelectorAll('.search-dropdown-list-wrapper-applicationui').forEach(target => target.classList.remove('hide'));
                    document.querySelectorAll('.search-dropdown-list-wrapper-marketing').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.search-dropdown-list-wrapper-ecommerce').forEach(target => target.classList.add('hide'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-applicationui').forEach(target => target.classList.add('active'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-ecommerce').forEach(target => target.classList.remove('active'));
                    document.querySelectorAll('.navbar_dropdown-tab-link-marketing').forEach(target => target.classList.remove('active'));
                });
            });
        });
    </script>

    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-K67VS8Q"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.4/gsap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.12.4/Flip.min.js"></script>
    <script>
        gsap.registerPlugin(Flip);

        const shuffleButton = document.querySelector('.new-home_header_shuffle-button');

        // elements
        const images = document.querySelectorAll('.new-home_header_image');
        const heading = document.querySelector('.new-home_header_heading');
        const subheading = document.querySelector('.new-home_header_subheading');
        const badge = document.querySelector('.new-home_header_badge');

        // state one containers
        const oneLeft = document.querySelector('.new-home_header_one-left');
        const oneCenter = document.querySelector('.new-home_header_one-center');
        const oneRight = document.querySelector('.new-home_header_one-right');

        // state two containers
        const twoTopLeft = document.querySelector('.new-home_header_two-left');
        const twoTopRight = document.querySelector('.new-home_header_two-right');
        const twoBottom = document.querySelector('.new-home_header_two-bottom');

        // state three containers
        const threeLeft = document.querySelector('.new-home_header_three-left');
        const threeRight = document.querySelector('.new-home_header_three-right');
        const testimonial = document.querySelector('.new-home_header_three-testimonial');

        // cursors
        const jessica = document.querySelector('.new-home_header_jessica');
        const mario = document.querySelector('.new-home_header_mario');

        gsap.set(testimonial, {
            y: '100%'
        });

        let currentState = 0;

        window.Webflow || = [];
        window.Webflow.push(() => {
            shuffleButton.addEventListener('click', () => {
                // Record the state of components
                const state = Flip.getState(
                    [...images, heading, subheading, badge, testimonial, jessica, mario], {
                        props: 'color,fontSize,textAlign,opacity',
                    }
                );

                // Perform DOM manipulations
                if (currentState === 0) {
                    // go to state 2
                    currentState = 1;
                    images.forEach((image) => {
                        image.classList.add('state-two');
                        twoBottom.append(image);
                    });
                    heading.classList.add('state-two');
                    subheading.classList.add('state-two');
                    jessica.classList.add('state-two');
                    mario.classList.add('state-two');

                    twoTopLeft.append(badge);
                    twoTopLeft.append(heading);
                    twoTopRight.append(subheading);
                } else if (currentState === 1) {
                    // go to state 3
                    currentState = 2;

                    images.forEach((image, index) => {
                        image.classList.remove('state-two');
                        if (index === 4) {
                            image.classList.add('state-three');
                        } else {
                            image.classList.add('state-hidden');
                        }
                    });

                    heading.classList.remove('state-two');
                    heading.classList.add('state-three');

                    subheading.classList.remove('state-two');
                    subheading.classList.add('state-three');

                    testimonial.classList.remove('state-hidden');
                    gsap.set(testimonial, {
                        y: '0%'
                    });

                    threeRight ? .append(images[4]);
                    threeLeft ? .append(badge);
                    threeLeft ? .append(heading);
                    threeLeft ? .append(subheading);

                    jessica.classList.remove('state-two');
                    jessica.classList.add('state-three');

                    mario.classList.remove('state-two');
                    mario.classList.add('state-three');
                } else {
                    // go to state 1
                    currentState = 0;
                    images.forEach((image, index) => {
                        if (index === 4) {
                            image.classList.remove('state-three');
                        } else {
                            image.classList.remove('state-hidden');
                        }
                        if (index < 3) {
                            oneLeft ? .append(image);
                        } else {
                            oneRight ? .append(image);
                        }
                    });

                    heading.classList.remove('state-three');

                    subheading.classList.remove('state-three');

                    testimonial.classList.add('state-hidden');
                    gsap.set(testimonial, {
                        y: '100%'
                    });

                    oneCenter ? .append(badge);
                    oneCenter ? .append(heading);
                    oneCenter ? .append(subheading);

                    jessica.classList.remove('state-three');
                    mario.classList.remove('state-three');
                }

                // Animate with Flip
                Flip.from(state, {
                    duration: 0.4,
                    absolute: [heading, subheading, badge, jessica, mario],
                    simple: true,
                    ease: 'back.out(1)',
                });
            });
        });
    </script>
</body>

</html>
