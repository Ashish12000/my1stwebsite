                                                        c.style.borderRight = "initial";
                                                        c.style[q[a]] = z;
                                                        c.style[r[a]] = A;
                                                        c.style[b] = "auto";
                                                        c.style[a] = -B + "px"
                                                    },
                                                    g = f.parentElement.querySelector("." + tooltipWrapperClass),
                                                    c = f.parentElement.querySelector("." + arrowClass),
                                                    h = c.classList.contains("is-left") ? "left" : c.classList.contains("is-right") ?
                                                    "right" : c.classList.contains("is-bottom") ? "bottom" : "top",
                                                    v = t[h],
