                                        };
                                    var k = {
                                        start: "left",
                                        end: "right",
                                        g: "width",
                                        translate: "translateX"
                                    };
                                    const u = {
                                            start: "top",
                                            end: "bottom",
                                            g: "height",
                                            translate: "translateY"
                                        },
                                        y = {
                                            bottom: k,
                                            left: u,
                                            right: u,
                                            top: k
                                        },
                                        n = f => "auto" === f ? "9999999px" : f,
                                        D = f => {
                                            if (!f.dataset.relumeTooltipSetup) {
                                                f.dataset.relumeTooltipSetup = 1;
                                                var C = a => {
                                                        const b = t[a];
                                                        g.style[b] = "100%";
                                                        g.style[a] = "auto";
                                                        g.style["margin" + (b[0].toUpperCase() + b.slice(1))] = l + "px";
                                                        g.style["margin" + (a[0].toUpperCase() + a.slice(1))] = "0";
                                                        c.style.borderTop = "initial";
                                                        c.style.borderLeft = "initial";
