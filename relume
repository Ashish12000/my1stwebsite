        if (typeof Cookies.get(cookieTwo) === 'undefined') {
            $('.anouncement-popup_component').css('display', 'flex');
        }

        $('.anouncement-close').on('click', function() {
            Cookies.set(cookieTwo, 'ok', {
                expires: 30
