                                                    l = parseInt(d.marginTop, 10) || parseInt(d.marginBottom, 10) || parseInt(d.marginLeft, 10) || parseInt(d.marginRight, 10) || 0;
                                                d = window.getComputedStyle(c);
                                                var B = -Math.min(parseInt(n(d.top), 10), parseInt(n(d.bottom), 10), parseInt(n(d.left), 10), parseInt(n(d.right), 10), 0),
                                                    z = d[q[h]],
                                                    A = d[r[h]],
                                                    p = !1,
                                                    w = () => {
                                                        if (p) {
                                                            window.requestAnimationFrame(w);
                                                            var a = g.getBoundingClientRect(),
                                                                b = f.getBoundingClientRect(),
                                                                e = {
                                                                    bottom: b.bottom + l + a.height <
                                                                        window.innerHeight,
                                                                    left: 0 < b.left - l - a.width,
